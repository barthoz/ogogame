<?xml version="1.0" encoding="UTF-8"?>
<nifty xmlns="http://nifty-gui.sourceforge.net/nifty-1.3.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://nifty-gui.sourceforge.net/nifty-1.3.xsd http://nifty-gui.sourceforge.net/nifty-1.3.xsd">
     <!-- Your IDE now tells you that one <screen></screen> element is expected here, etc. -->
    
    <useControls filename="nifty-default-controls.xml" />
  <useStyles filename="nifty-default-styles.xml" />
    
    <screen id="hud" controller="main.game.gui.HudController">
        <layer id="layer1" backgroundColor="#0000" childLayout="center">
            <panel id="panel_actions" height="25%" width="25%" align="right" valign="bottom" backgroundColor="#f60f" childLayout="center" visibleToMouse="true">
                <control name="button" label="Move" id="StartButton" align="center" valign="center" visibleToMouse="true" >
                    <interact onClick="test(hud)"/>
                </control>
                <control name="button" label="Eat" id="StartButton" align="center" valign="center" visibleToMouse="true" >
                    <interact onClick="test(hud)"/>
                </control>
                <control name="button" label="Attack" id="StartButton" align="center" valign="center" visibleToMouse="true" >
                    <interact onClick="test(hud)"/>
                </control>
            </panel>
        </layer>
  </screen>
</nifty>